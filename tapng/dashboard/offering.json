{
  "template": {
    "body": {
      "componentType": "instance",
      "persistentVolumeClaims": null,
      "deployments": [
        {
          "kind": "Deployment",
          "apiVersion": "extensions\/v1beta1",
          "metadata": {
            "name": "gp-ui-$idx_and_short_instance_id",
            "creationTimestamp": null,
            "labels": {
              "plan_id": "$plan_id",
              "offering_id": "$offering_id",
              "idx_and_short_instance_id": "$idx_and_short_instance_id",
              "managed_by": "TAP",
              "org": "$org",
              "instance_id": "$instance_id",
              "space": "$space"
            }
          },
          "spec": {
            "replicas": 1,
            "selector": {
              "matchLabels": {
                "idx_and_short_instance_id": "$idx_and_short_instance_id",
                "instance_id": "$instance_id"
              }
            },
            "template": {
              "metadata": {
                "creationTimestamp": null,
                "labels": {
                  "idx_and_short_instance_id": "$idx_and_short_instance_id",
                  "managed_by": "TAP",
                  "instance_id": "$instance_id"
                }
              },
              "spec": {
                "volumes": null,
                "containers": [
                  {
                    "name": "gearpump-dashboard",
                    "image": "127.0.0.1:30000/gearpump-dashboard:0.1.0",
                    "ports": [
                      {
                        "containerPort": 80,
                        "protocol": "TCP"
                      }
                    ],
                    "env": [
                      {
                        "name": "MANAGED_BY",
                        "value": "TAP"
                      },
                      {
                        "name": "UAA_API_ENDPOINT",
                        "valueFrom": {
                          "configMapKeyRef": { "name": "sso", "key": "api-endpoint" }
                        }
                      },
                      {
                        "name": "UAA_HOST",
                        "value": "http://uaa.$domain_name"
                      },
                      {
                        "name": "CALLBACK",
                        "value": "$hostname.$domain_name"
                      },
                      {
                        "name": "UAA_SCOPE",
                        "value": "openid,tap.user"
                      }
                    ],
                    "imagePullPolicy": "IfNotPresent"
                  }
                ],
                "restartPolicy": "Always",
                "dnsPolicy": "ClusterFirst",
                "serviceAccountName": ""
              }
            },
            "strategy": {
            }
          },
          "status": {
          }
        }
      ],
      "ingresses": [
        {
          "kind": "Ingress",
          "apiVersion": "extensions/v1beta1",
          "metadata": {
            "name": "gp-ui-$idx_and_short_instance_id",
            "creationTimestamp": null,
            "labels": {
              "plan_id": "$plan_id",
              "offering_id": "$offering_id",
              "idx_and_short_instance_id": "$idx_and_short_instance_id",
              "managed_by": "TAP",
              "org": "$org",
              "instance_id": "$instance_id",
              "space": "$space"
            }
          },
          "spec": {
            "rules": [
              {
                "host": "$hostname.$domain_name",
                "http": {
                  "paths": [
                    {
                      "path": "/",
                      "backend": {
                        "serviceName": "gp-ui-$idx_and_short_instance_id",
                        "servicePort": 80
                      }
                    }
                  ]
                }
              }
            ]
          },
          "status": {
            "loadBalancer": {}
          }
        }
      ],
      "services": [
        {
          "kind": "Service",
          "apiVersion": "v1",
          "metadata": {
            "name": "gp-ui-$idx_and_short_instance_id",
            "creationTimestamp": null,
            "labels": {
              "plan_id": "$plan_id",
              "offering_id": "$offering_id",
              "idx_and_short_instance_id": "$idx_and_short_instance_id",
              "managed_by": "TAP",
              "org": "$org",
              "instance_id": "$instance_id",
              "space": "$space"
            }
          },
          "spec": {
            "type": "NodePort",
            "ports": [
              {
                "name": "rest",
                "protocol": "TCP",
                "port": 80,
                "targetPort": 0,
                "nodePort": 0
              }
            ],
            "selector": {
              "instance_id": "$instance_id"
            }
          },
          "status": {
            "loadBalancer": {
            }
          }
        }
      ],
      "serviceAccounts": [
        {
          "kind": "ServiceAccount",
          "apiVersion": "v1",
          "metadata": {
            "name": "gp-ui-$idx_and_short_instance_id",
            "creationTimestamp": null,
            "labels": {
              "plan_id": "$plan_id",
              "offering_id": "$offering_id",
              "idx_and_short_instance_id": "$idx_and_short_instance_id",
              "managed_by": "TAP",
              "org": "$org",
              "instance_id": "$instance_id",
              "space": "$space"
            }
          },
          "secrets": null
        }
      ],
      "secrets": null
    },
    "hooks": null
  },
  "services": [{
    "name": "gearpumpdashboard",
    "description": "The GearPump dashboard service instantiated by the GearPump broker.",
    "metadata": [
      {
        "key": "source_offering_id",
        "value": "gearpump-dashboard-id"
      },
      {
        "key": "source_plan_id-free",
        "value": "gearpump-dashboard-plan-id"
      }
    ],
    "bindable": false,
    "tags": [
      "gearpump",
      "gearpump-dashboard"
    ],
    "plans": [
      {
        "name": "free",
        "description": "free",
        "cost": "free"
      }
    ]
  }]
}
